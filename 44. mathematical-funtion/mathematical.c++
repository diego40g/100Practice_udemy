#include <iostream>
#include <cmath>

using namespace std;

int main(){
    int x=100,y=-23;
    float a=56.8,b=100;

    cout<<"Funciones matematicas\n"<<endl;

    cout<<"sin(x) = "<<sin(x)<<endl;
    cout<<"log(x) = "<<log(x)<<endl;
    cout<<"abs(y) = "<<abs(y)<<endl;
    cout<<"floor(a) = "<<floor(a)<<endl;
    cout<<"sqrt(b) = "<<sqrt(b)<<endl;
    cout<<"pow(b,2) = "<<pow(b,2)<<endl;

    return 0;
}
